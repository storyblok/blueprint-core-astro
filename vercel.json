{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"framework": "astro",
	"trailingSlash": false,
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Content-Security-Policy",
					"value": "frame-ancestors 'self' https://app.storyblok.com"
				}
			]
		}
	]
}
